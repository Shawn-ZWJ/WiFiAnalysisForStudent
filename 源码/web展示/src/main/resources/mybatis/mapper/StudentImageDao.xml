<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="cn.edu.dao.StudentImageDao">

    <select id="getStuImgByMac" resultType="StudentImageBean">
        SELECT * FROM student_image_${year} WHERE mac = #{mac}
    </select>

</mapper>